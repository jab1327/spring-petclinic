pipeline {
    agent any

    stages {
        stage('Check out') {
            steps {
                // Get some code from a GitHub repository
                git branch: 'main', url: 'https://github.com/xaviervaucluse/spring-petclinic'
                
            }

            }
        stage('Build') {
            steps {
                
                sh 'echo "BUILD"'
               
       
            }

           }
        }
    }

